# Used by docker-compose.yml
ELEGANTMC_DAEMON_ID=my-node
ELEGANTMC_TOKEN=devtoken123
ELEGANTMC_PANEL_PORT=3000
ELEGANTMC_PANEL_ADMIN_PASSWORD=change-me

# Optional (recommended in China)
# NPM_REGISTRY=https://registry.npmmirror.com
# GOPROXY=https://goproxy.cn,direct
# GOSUMDB=sum.golang.google.cn
# ELEGANTMC_MOJANG_META_BASE_URL=
# ELEGANTMC_MOJANG_DATA_BASE_URL=

# Optional (Java selection)
# ELEGANTMC_JAVA_CANDIDATES=java

# Optional (when Panel is behind HTTPS reverse proxy)
# ELEGANTMC_PANEL_SECURE_COOKIE=1
# ELEGANTMC_PANEL_HSTS=1

# Optional (DockerHub / prebuilt images via docker-compose.images.yml)
# ELEGANTMC_PANEL_IMAGE=YOUR_DOCKERHUB_USER/elegantmc-panel
# ELEGANTMC_DAEMON_IMAGE=YOUR_DOCKERHUB_USER/elegantmc-daemon
# ELEGANTMC_IMAGE_TAG=latest

# Optional (dangerous)
# ELEGANTMC_ENABLE_ADVANCED=1
